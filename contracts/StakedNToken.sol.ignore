
contract StakedNToken is ERC20, BoringOwnable, Initializable, UUPSUpgradeable {
    ERC20 public immutable underlyingNToken;
    NotionalProxy public immutable NOTIONAL;

    constructor(
        ERC20 _stNOTE,
        NotionalProxy _notional
    ) ERC20("Vested Staked NOTE", "vestNOTE") initializer { 
        stNOTE = _stNOTE;
        NOTIONAL = _notional;
    }

}